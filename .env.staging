# Middleware V2 Configuration
# البيئة: Staging (معزولة للاختبار)

# Middleware Settings
MW_BASE_URL=http://localhost:8080
MW_PORT=8080
NODE_ENV=staging

# Backend Settings
BE_BASE_URL=https://api.mmc-mms.com/api/v1

# Feature Flags
FEATURE_MW_ENABLED=true

# SSE/WebSocket
SSE_CHANNEL=/mw/admin/live

# Security (للاختبار المحلي فقط)
MW_SECRET_KEY=staging_test_key_not_for_production

# Logging
LOG_LEVEL=debug
LOG_TO_FILE=true

